@{
    ViewBag.Title = "Check Aadhar Card";
    Layout = "~/Views/Shared/_Layout-Admin.cshtml";
}

<center><h2>Aadhar Card Validator</h2></center>
<hr />
<div class="container">
    <p style="text-align:center">
        <strong>@ViewBag.message</strong>
        <br />
        <b>  Entered Aadhar Number: </b><strong> @Session["enterednum"]</strong>
        <br />
       <b> Scanned Aadhar Number: </b><strong> @Session["aadharnum"]</strong>
        <br />
       
    </p>
    @Html.ActionLink("Go to Previous Page","ViewAadhar",new{ id= Convert.ToInt32(Session["mm"])})
</div>
<hr />

